{
  "name": "test-package-leszek",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root client/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "buildDev": "vite build --mode development",
    "buildProd": "vite build --mode production",
    "serve": "vite serve"
  },
  "dependencies": {
    "pinia": "^2.0.26",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@aem-vite/import-rewriter": "^7.0.0",
    "@aem-vite/vite-aem-plugin": "^2.3.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "terser": "^5.16.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^4.0.0",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  }
}
